{
    "name": "laminblur/filament-pdf-sanitizer",
    "description": "A Filament plugin that automatically sanitizes PDF files by removing JavaScript, forms, and annotations before upload to prevent WAF blocking",
    "type": "library",
    "version": "1.0.9",
    "license": "MIT",
    "authors": [
        {
            "name": "laminblur",
            "email": "laminblur@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Laminblur\\FilamentPdfSanitizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laminblur\\FilamentPdfSanitizer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laminblur\\FilamentPdfSanitizer\\FilamentPdfSanitizerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
